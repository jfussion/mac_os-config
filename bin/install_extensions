#! /usr/bin/env bash

# DESCRIPTION
# Installs application extensions.

# SETUP

# EXECUTION

# Vim - Pathogen
install_file "$VIM_PATHOGEN_EXTENSION_URL" "$VIM_PATHOGEN_EXTENSION_PATH"

# Vim - Projectionist
install_git_app "$VIM_PROJECTIONIST_EXTENSION_URL" "$VIM_PROJECTIONIST_EXTENSION_PATH"

# Vim - Splitjoin
install_git_app "$VIM_SPLITJOIN_EXTENSION_URL" "$VIM_SPLITJOIN_EXTENSION_PATH"

